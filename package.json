{
  "name": "uikit-product",
  "description": "Cloud.ru Product Uikit",
  "homepage": "https://cloud-ru-tech.github.io/uikit-product/",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloud-ru-tech/uikit-product/tree/master"
  },
  "license": "Apache-2.0",
  "scripts": {
    "add-package": "ts-node scripts/npm-init",
    "build:storybook": "pnpm install:themes && ts-node scripts/build-storybook-changed",
    "build:storybook:all": "cross-env TEST_SERVER=true storybook build -c storybook",
    "build:packages:esm": "tspc -b ./packages/tsconfig.esm.json",
    "build:packages:cjs": "tspc -b ./packages/tsconfig.cjs.json",
    "compile:packages": "lerna run compile",
    "build:packages": "ts-node scripts/build-packages",
    "build:css": "ts-node scripts/compileCSS",
    "changelog": "ts-node scripts/changelog",
    "clean:all": "pnpm clean:empty && pnpm clean:dist && pnpm clean:modules && pnpm clean:buildinfo",
    "clean:dist": "rimraf --glob ./packages/*/dist",
    "clean:modules": "rimraf --glob ./packages/*/node_modules",
    "clean:buildinfo": "rimraf --glob ./packages/**/*.tsbuildinfo ./packages/npm-package.tsconfig.tsbuildinfo",
    "clean:empty": "ts-node scripts/removeEmptyPackages.ts",
    "docgen": "ts-node scripts/docgen/docgenForAllPackages",
    "deps": "pnpm install",
    "deps:reinstall": "pnpm clean:all && rm -rf node_modules pnpm-lock.yaml && pnpm store prune && pnpm deps",
    "storybook:all": "pnpm install:themes && storybook dev -c storybook -p 6006 --https --ssl-cert ../cert.crt --ssl-key ../cert.key",
    "storybook:partial": "ts-node scripts/start-storybook-partially",
    "test:ci": "ts-node scripts/run-tests",
    "test:e2e:chrome": "playwright test --project=chrome --pass-with-no-tests",
    "test:e2e:firefox": "playwright test --project=firefox --pass-with-no-tests",
    "test:local": "cross-env TEST_LOCAL=true playwright test --ui",
    "test:unit": "ts-node scripts/run-unit-tests",
    "install:themes": "ts-node scripts/install-additional-themes"
  },
