  "scripts": {
    "add-package": "ts-node scripts/npm-init",
    "build:storybook": "cross-env TEST_SERVER=true storybook build -c storybook",
    "build:packages": "pnpm clean:all && pnpm deps:packages && lerna run compile && ts-node scripts/compile",
    "changelog": "ts-node scripts/changelog",
    "clean:all": "pnpm clean:dist && pnpm clean:modules",
    "clean:dist": "rimraf \"./packages/**/dist/\"",
    "clean:empty": "ts-node scripts/removeEmptyPackages.ts",
    "clean:modules": "rimraf \"./packages/**/node_modules/\"",
    "docgen": "ts-node scripts/docgen/docgenForAllPackages",
    "deps:all": "pnpm deps:global && pnpm deps:packages",
    "deps:global": "pnpm i --shamefully-hoist",
    "deps:packages": "pnpm install",
    "deps:reinstall": "pnpm clean:all && rm -rf node_modules pnpm-lock.yaml && pnpm store prune && pnpm deps:all",
    "storybook:all": "storybook dev -c storybook -p 6006 --https --ssl-cert ../cert.crt --ssl-key ../cert.key",
    "storybook:partial": "ts-node scripts/start-storybook-partially",
    "test:coverage-all": "ts-node scripts/test-coverage-all-packages",
    "test:coverage-unstable": "ts-node scripts/test-coverage-unstable-packages",
    "test:coverage-stable": "ts-node scripts/test-coverage-stable-packages",
    "test:ci": "ts-node scripts/run-tests",
    "test:testcafe": "testcafe chrome --config-file testcafe.config.js --live",
    "test:testcafe:local": "cross-env TEST_LOCAL=true pnpm test:testcafe",
    "test:testcafe:branch": "cross-env TEST_ON_BRANCH=true pnpm test:testcafe",
    "test:unit": "vitest run"
  },
