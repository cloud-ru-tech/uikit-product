{
  "name": "@sbercloud/uikit2.0",
  "version": "2.0.0",
  "description": "SberCloud UI-Kit",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "license": "UNLICENSED",
  "scripts": {
    "prepack": "node scripts/prepublish.js",
    "storybook": "yarn start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "clean": "yarn rimraf dist && node scripts/clean.js",
    "build": "yarn run-s clean build:types build:es build:cjs",
    "build:watch": "yarn run-p build:types:watch build:es:watch build:cjs:watch",
    "build:es": "cross-env BUILD_TYPE=es yarn babel src --out-dir dist/esm --extensions '.ts,.tsx' --copy-files fonts",
    "build:es:watch": "yarn build:es -- --watch",
    "build:types": "yarn tsc -p tsconfig.types.json",
    "build:types:watch": "yarn build:types -- -w",
    "build:cjs": "cross-env BUILD_TYPE=cjs yarn babel src --out-dir dist/cjs --extensions '.ts,.tsx'",
    "build:cjs:watch": "yarn build:cjs -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/preset-env": "^7.13.15",
    "@ag-grid-community/react": "25.1.0",
    "@ag-grid-enterprise/all-modules": "25.1.0",
    "@sbercloud/icons": "1.0.15-14c9f91b",
    "ag-grid-community": "25.1.0",
    "ag-grid-enterprise": "25.1.0",
    "ag-grid-react": "25.1.0",
    "date-fns": "2.21.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "nanoid": "^3.1.22",
    "rc-drawer": "^4.3.1",
    "rc-slider": "^9.7.1",
    "rc-tree": "^4.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-input-autosize": "^3.0.0",
    "react-modal": "^3.12.1",
    "react-paginate": "^7.1.0",
    "react-popper-tooltip": "3.1.0",
    "react-select": "3.1.0",
    "react-switch": "^6.0.0",
    "react-textarea-autosize": "^8.3.2",
    "react-toastify": "^7.0.3"
  },
