{
  "name": "@sbercloud/uikit2.0",
  "description": "Sbercloud UI-Kit",
  "license": "UNLICENSED",
  "scripts": {
    "add-package": "ts-node scripts/npm-init",
    "build:storybook": "build-storybook",
    "build:packages": "npm run clean:all && npm run deps:packages && lerna run compile && ts-node scripts/compile.ts",
    "changelog": "ts-node scripts/changelog.ts",
    "clean:all": "npm run clean:dist && npm run clean:modules",
    "clean:dist": "rimraf \"./packages/**/dist/\"",
    "clean:modules": "rimraf \"./packages/**/node_modules/\"",
    "deps:all": "npm run deps:global && npm run deps:packages",
    "deps:global": "npm i",
    "deps:packages": "npm i --workspaces",
    "deps:reinstall": "rm -rf node_modules package-lock.json && npm cache clean -f && npm run deps:all",
    "storybook:all": "start-storybook -p 6006",
    "storybook:ci": "start-storybook -p 6006 --ci --quiet --loglevel warn",
    "storybook:partial": "ts-node scripts/start-storybook-partially.ts",
    "test:coverage-all": "ts-node scripts/test-coverage-all-packages.ts",
    "test:coverage-unstable": "ts-node scripts/test-coverage-unstable-packages.ts",
    "test:coverage-stable": "ts-node scripts/test-coverage-stable-packages.ts",
    "test:cypress": "ts-node scripts/run-tests.ts",
    "test:ci": "cross-env NODE_ENV=test start-server-and-test storybook:ci http://localhost:6006 test:cypress"
  },
