{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "pnpm": [
      {
        "rule": "cli",
        "binary": "pnpm",
        "semver": "^10.22.0",
        "error": "You have pnpm@{{installedVersion}}, and need pnpm@{{wantedVersion}}. Fix with `pnpm self-update {{wantedVersion}}`"
      }
    ],
    "NPM": [
      {
        "rule": "cli",
        "binary": "npm",
        "semver": "^11.6.2",
        "version": "--version",
        "error": "You have npm@{{installedVersion}}, and need npm@{{wantedVersion}}. Fix with `npm install -g npm@{{wantedVersion}}`"
      }
    ],
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": "^24.11.1",
        "version": "--version",
        "error": "You have node@{{installedVersion}}, and need node@{{wantedVersion}}. Go to https://nodejs.org/ to update`"
      }
    ],
    "Git email": [
      {
        "rule": "shell",
        "command": "git config user.email",
        "match": ".+@cloud\\.ru",
        "error": "You have an incorrect email set in git. Execute `git config --global user.email {your-name}@cloud.ru`"
      }
    ]
  }
}
